    "consola": "^3.4.0",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.38.3",
    "drizzle-zod": "^0.6.1",
    "geojson": "^0.5.0",
    "gtfs": "^4.15.12",
    "maplibre-gl": "^5.1.0",
    "masto": "^6.10.2",
    "mitt": "^3.0.1",
    "sanitize-html": "^2.14.0",
