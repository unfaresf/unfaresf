    "@types/sanitize-html": "^2.13.0",
    "@types/serviceworker": "^0.0.119",
    "@types/web-push": "^3.6.4",
    "@vite-pwa/assets-generator": "^0.2.6",
    "nuxt": "^3.6.0",
    "nuxt-auth-utils": "^0.5.7",
    "nuxt-authorization": "^0.3.2",
    "nuxt-cron": "^1.8.0",
    "nuxt-rate-limit": "^1.2.0",
    "sass": "^1.85.0",
    "typescript": "^5.7.3",
