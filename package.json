{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "dev": "npx nuxi@latest dev --no-fork",
    "dev:host": "npx nuxi@latest dev --no-fork --host 0.0.0.0",
    "dev:inspect": "npx nuxi@latest dev --inspect",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "gtfs:init": "node ./gtfs/init.js",
    "gtfs:migrate": "drizzle-kit migrate --config=drizzle-gtfs.config.ts"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/ui": "^2.20.0",
    "@nuxtjs/device": "^3.2.4",
